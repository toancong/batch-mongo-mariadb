{
    "name": "jinjer/batch-mongo-maria",
    "description": "Batch convert collections from mongodb to mariadb",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "vlucas/phpdotenv": "^2.4",
        "mongodb/mongodb": "^1.1",
        "robmorgan/phinx": "^0.7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/var-dumper": "^3.2"
    },
    "autoload": {
        "psr-4": {
              "BatchMongoRDB\\": "src/BatchMongoRDB"
        },
        "classmap": ["tests/"]
    },
    "scripts": {
      "test": [
          "phpunit --colors=always"
      ]
    }
}
